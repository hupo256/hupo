/*  rerest  */
html,body{
	height:100%;
	min-height:100%;	
}
body{
	font-family: "微软雅黑", "宋体", "黑体";
	font-size: 12px;
	line-height: 1.8;
	color:#434343;	
}
article, aside, details, figcaption, figure, footer, header, hgroup, menu, nav, section{display:block}
body,h1,h2,h3,p,ul,ol,dl,dd,menu{margin:0;}
input,textarea,select{font-size:100%;font-family:inherit;}
textarea{resize:none; padding:3px 5px;}
ul,ol,menu{padding-left:0;list-style:none;}
a{text-decoration:none; color:#454545;}
a:hover{color:#16a;}
img{border:0;}
u{font-style:normal;}
b{font-weight:400;}
button{border:none; cursor:pointer;}
input,textarea{border:1px solid #ccc;}
table{width:100%;border-collapse: collapse; border-spacing: 0; border:1px solid #ddd;}
th,td{border:1px solid #ccc; height:30px; padding:0 5px; line-height:30px;}
td{border:none;}
h3{font-size:18px; font-weight:800;}
.fr{float:right;}
.fl{float:left;}
.fb{font-weight:800;}
.f18{font-size:18px;}
.ptb70{padding-top:50px; padding-bottom:50px;}
.pt70{padding-top:50px;}
.pb70{padding-bottom:50px;}
.ptb30{padding-top:30px; padding-bottom:30px;}
.pt30{padding-top:30px;}
.pb30{padding-bottom:30px;}
.pr15{padding-right:15px;}
.plr{padding:0 5px;}
.border{border:1px solid #ccc;}
.border_t{border-top:1px solid #ddd;}
.border_r{border-right:1px solid #ddd;}
.border_b{border-bottom:1px solid #ddd;}
.border_tb{border-top:1px solid #ddd; border-bottom:1px solid #ddd;}
.border_non{border:none;}
.dib{display:inline-block;vertical-align:top;}
.dib{*display:inline;}
.tac{text-align:center;}
.tal{text-align:left;}
.tar{text-align:right;}
.vam{vertical-align:middle;}
.fcr{color:#c00;}
.fcg{color:#46c340;}
.fcb{color:#4691F6;}
.fccc{color:#aaa;}
.bgc{background:#f6f6f6; min-width:980px;}
.bgf0{background:#f0f0f0;min-width:980px;}
.d_none{display:none;}
.p_rlt{position:relative;}
.p_abs{position:absolute;}
.ell{	
	text-overflow:ellipsis; 
	white-space:nowrap; 
	overflow:hidden;
}
.fix{*zoom:1;}
.fix:after{	
	content:".";	
	clear:both;	
	height:0;
	visibility:hidden;
	display:block;
}
.bor{
	display:inline-block;		
	border-width:6px 5px;
	border-style:solid;
	border-color:#ddd transparent transparent;
	position:absolute;
	width:0;
	height:0;
}
.btnbox{text-align:center;}
.btn{
	border-radius:2px;
	background:#4691f6;
	color:#fff;
	padding:3px 15px;
	display:inline-block;
}
.btn:hover{
	background:#1962c4;
	color:#fff;
}
.inp_tex{
	border-radius:2px;
	padding:5px 3px;
	height:15px;
}
.gtlt {
    cursor: default;
    font-family: simsun;
    letter-spacing: -0.1em;
	padding:0 5px;
}
.ico,
.totop,
.logo_l,
.logo a{
	display:inline-block;
	background:url(../images/icons.png) no-repeat;
	overflow:hidden;
	vertical-align:middle;
}
.io_edit,
.io_save,
.io_delete,
.io_delete_on{
	width:14px;
	height:14px;
	vertical-align:middle;
	margin-right:7px;
	cursor:pointer;
}
.io_edit{background-position:-567px 0;}
.io_save{background-position:-595px -14px;}
.io_delete{background-position:-581px 0;}
.io_delete_on{background-position:-581px -14px;}
.io_edit:hover{background-position:-567px -14px;}
.io_delete:hover{background-position:-581px -14px;}
.io_select{
	background-position:-780px -167px;
	width:29px;
	height:29px;
	border-left:1px solid #ddd;
	margin-left:-30px;
	cursor:pointer;
}


.headbox,
.summarybox,
.sumbox, 
.featurebox, 
.appbox, 
.freashbox,
.contentbox, 
.footer{
	width:980px;
	margin:0 auto;
	overflow:hidden;
}


