<div id="totop" class="totop"></div>
	
<!--  弹层 选择方法分类  -->
<div class="popuptbox" id="staffSelect">
	<div class="p10">
		<h3 class="pop_tit border_b">
			选择联系人
			<a href="#" class="ico io_close close fr"></a>
		</h3>	
		
		<ul class="pop_slebox fix">			
			<li class="servebox">
				<input class="inp_tex" type="text" placeholder="输入姓名/工号/邮件地址"><u class="ico io_search"></u>			
				<table>
					<tr class="bgc">
						<th width="20%">姓名</th>
						<th width="30%">部门</th>
						<th width="50%">邮箱地址</th>
					</tr>
				</table>
				
				<div class="servetb">
					<div class="mail_tbox">
						<ul class="serve_list">							
							<!-- <li>
								<b class="ell">王阳</b>
								<b class="ell">IT工程部</b>
								<b class="ell">snowfay@gmail.com</b>							
							</li>
							 -->
						</ul>
					</div>
				</div>
			</li>

			<li class="selec_toright"></li>
			
			<li class="resultbox">
				<p class="check_num">已经添加邮箱<b class="fb">5</b>个</p>
				<table>
					<tr class="bgc">
						<th width="19%">姓名</th>
						<th width="25%">部门</th>
						<th width="45%">邮箱地址</th>
						<th width="11%">操作</th>
					</tr>
				</table>
				
				<div class="resultb">
					<div class="mail_tbox">
						<ul class="selected_list">							
							<li>
								<b class="ell">王阳</b>
								<b class="ell">IT工程部</b>
								<b class="ell">snowfay@gmail.com</b>							
								<b><u class="ico io_delete"></u></b>							
							</li>							
						</ul>
					</div>
				</div>
			</li>
			
		</ul>
			
		<div class="btnbox">			
			<button class="d_none close">click me</button>
			<a href="#" class="btn close" id="">取消</a>
			<a href="#" class="btn" id="">确定</a>
		</div>
		
	</div>	
