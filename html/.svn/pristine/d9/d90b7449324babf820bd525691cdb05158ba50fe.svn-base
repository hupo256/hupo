}
.fix{*zoom:1;}
.fix:after{	
	content:".";	
	clear:both;	
	height:0;
	visibility:hidden;
	display:block;
}
.bor{
	display:inline-block;		
	border-width:6px 5px;
	border-style:solid;
	border-color:#ddd transparent transparent;
	position:absolute;
	width:0;
	height:0;
}
.btnbox{text-align:center;}
.btn{
	border-radius:2px;
	background:#4691f6;
	color:#fff;
	padding:3px 15px;
